configuration:
  execInParallel: false
  gnbs:
    gnb1:
      defaultAmf:
        hostName: {{ amf_hostname }}
        port: {{ amf_port }}
      globalRanId:
        gNbId:
          bitLength: 24
          gNBValue: "000102"
        plmnId:
          mcc: {{ mcc }}
          mnc: {{ mnc }}
      n2Port: 9487
      n3IpAddr: {{ gnb_ip_address }}
      n3Port: 2152
      name: gnb1
      supportedTaList:
      - broadcastPlmnList:
        - plmnId:
            mcc: {{ mcc }}
            mnc: {{ mnc }}
          taiSliceSupportList:
          - sd: "{{ sd }}"
            sst: {{ sst }}
        tac: "{{ tac }}"
  goProfile:
    enable: false
    port: 5000
  httpServer:
    enable: false
  profiles:
  - profileType: register
    profileName: profile1
    enable: true
    gnbName: gnb1
    startImsi: {{ imsi }}
    ueCount: 1
    defaultAs: {{ icmp_packet_destination }}
    opc: {{ usim_opc }}
    key: {{ usim_key }}
    sequenceNumber: {{ usim_sequence_number }}
    dnn: {{ dnn }}
    sNssai:
      sst: {{ sst }}
      sd: "{{ sd }}"
    execInParallel: false
    plmnId:
      mcc: {{ mcc }}
      mnc: {{ mnc }}
  - profileType: pdusessest
    profileName: profile2
    dataPktCount: 5
    defaultAs: {{ icmp_packet_destination }}
    enable: true
    execInParallel: false
    gnbName: gnb1
    key: {{ usim_key }}
    opc: {{ usim_opc }}
    perUserTimeout: 100
    dnn: "internet"
    sNssai:
      sst: {{ sst }}
      sd: "{{ sd }}"
    plmnId:
      mcc: {{ mcc }}
      mnc: {{ mnc }}
    sequenceNumber: {{ usim_sequence_number }}
    startImsi: {{ imsi }}
    ueCount: 1
  - profileType: anrelease
    profileName: profile3
    enable: true
    gnbName: gnb1
    startImsi: {{ imsi }}
    ueCount: 1
    defaultAs: {{ icmp_packet_destination }}
    opc: {{ usim_opc }}
    key: {{ usim_key }}
    sequenceNumber: {{ usim_sequence_number }}
    dnn: {{ dnn }}
    sNssai:
      sst: {{ sst }}
      sd: "{{ sd }}"
    execInParallel: false
    plmnId:
      mcc: {{ mcc }}
      mnc: {{ mnc }}
  runConfigProfilesAtStart: true
info:
  description: gNodeB sim initial configuration
  version: 1.0.0
logger:
  logLevel: trace
